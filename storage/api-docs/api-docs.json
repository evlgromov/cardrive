  "paths": {
    "/cars": {
      "get": {
        "tags": [
          "Car"
        ],
        "summary": "Получение списка всех автомобилей",
        "description": "Метод возвращает список автомобилей из таблицы cars в формате json",
        "operationId": "getCarsList",
        "responses": {
          "200": {
            "description": "Successful operation"
          }
        }
      }
    },
    "/rents": {
      "get": {
        "tags": [
          "CarRent"
        ],
        "summary": "Получение списка всех занятых автомобилей",
        "description": "Метод возвращает список занятых автомобилей из таблицы car_rents в формате json",
        "operationId": "getCarRentsList",
        "responses": {
          "200": {
            "description": "Successful operation"
          }
        }
      },
      "post": {
        "tags": [
          "CarRent"
        ],
        "summary": "Сохранение аренды автомобиля пользователем",
        "description": "Возвращает сообщение об успешном сохранении",
        "operationId": "storeCarRent",
        "requestBody": {
          "required": true,
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "object",
                "properties": {
                  "user_id": {
                    "type": "integer"
                  },
                  "car_id": {
                    "type": "integer"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation"
          },
          "422": {
            "description": "Unprocessable Content"
          }
        }
      }
    },
    "/rents/{id}": {
      "get": {
        "tags": [
          "CarRent"
        ],
        "summary": "Информация об аренде",
        "description": "Возвращает информацию об аренде автомобиля по идентификатору",
        "operationId": "getCarRentById",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "Car rent id",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CarRent"
                }
              }
            }
          }
        }
      },
      "put": {
        "tags": [
          "CarRent"
        ],
        "summary": "Редактировать существующую аренду",
        "description": "Возвращает JSON с сообщением об успешном изменении",
        "operationId": "updateCarRent",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "Car rent id",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "type": "object",
                "properties": {
                  "user_id": {
                    "type": "integer"
                  },
                  "car_id": {
                    "type": "integer"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful operation"
          },
          "422": {
            "description": "Unprocessable Content"
          }
        }
      },
      "delete": {
        "tags": [
          "CarRent"
        ],
        "summary": "Удаление существующей аренды",
        "description": "Удаление существующей аренды и возврат JSON с сообщением об успешном удалении",
        "operationId": "deleteCarRent",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "Car rent id",
            "required": true,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Сообщение о выполнении"
          }
        }
      }
    },
    "/users": {
      "get": {
        "tags": [
          "User"
        ],
        "summary": "Получение списка всех пользователей",
        "description": "етод возвращает список пользователей из таблицы users в формате json",
        "operationId": "geUsersList",
        "responses": {
          "200": {
            "description": "Successful operation"
          }
        }
      }
    }
